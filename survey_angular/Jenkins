pipeline {
    agent any
    environment {
        PROJECT_ID = 'swe642'
    }
    stages {
        stage("Checkout code") {
            steps {
                checkout scm
            }
        }
        stage('build') {
            steps {
                sh 'ng build'
            	dir('survey_angular') {
            		echo 'Creating the Jar ...'
					sh 'ng build'
            	}
                
            }
        }
        
    }    
}